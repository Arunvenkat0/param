<?xml version="1.0" encoding="UTF-8" ?>
<testcase version="3" disabled="true" xmlns="http://xlt.xceptance.com/xlt-script/2">
  <description>Tests the navigation feature of the product catalog both on category and product details pages.

This is broken in 13.3 hence we disabled it. </description>
  <module name="modules.staticelements.Homepage"/>
  <action name="CheckCatalogNavigation"/>
  <module name="modules.catalog.CheckCatalogNavigationFeature">
    <parameter name="MainCategoryName" value="${MainCategoryName}"/>
    <parameter name="SubCategoryName" value="${SubCategoryName}"/>
    <parameter name="MiddlePageNumber" value="${MiddlePageNumber}"/>
  </module>
  <action name="BackToHome"/>
  <module name="modules.staticelements.Return2Home"/>
</testcase>
