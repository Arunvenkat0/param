<?xml version="1.0" encoding="UTF-8" ?>
<scriptmodule version="3" xmlns="http://xlt.xceptance.com/xlt-script/2">
  <description>Changes Name, Password and Phone</description>
  <parameter name="First_Name"/>
  <parameter name="Last_Name"/>
  <parameter name="Email"/>
  <parameter name="Password"/>
  <action name="OpenPersonalDataPage"/>
  <command name="clickAndWait" target="css=.account-options li:first-child a"/>
  <action name="Edit"/>
  <module name="modules.usermanagement.EnterRegisterInformation">
    <parameter name="FirstName" value="@{First_Name}"/>
    <parameter name="LastName" value="@{Last_Name}"/>
    <parameter name="EmailWrong" value="abc.de"/>
    <parameter name="EmailCorrect" value="@{Email}"/>
    <parameter name="PasswordTooShort" value="pass"/>
    <parameter name="PasswordCorrect" value="@{Password}"/>
  </module>
  <action name="SubmitRegistration"/>
</scriptmodule>
