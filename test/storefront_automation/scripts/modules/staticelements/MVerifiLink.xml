  <parameter name="ElementToVerify"/>
  <parameter name="UrlToPreviousPage"/>
  <parameter name="CheckTitle"/>
  <command name="waitForElementPresent" target="@{LinkToClick}"/>
  <command name="clickAndWait" target="@{LinkToClick}"/>
  <command name="assertTitle" value="@{CheckTitle}"/>
