<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="4">
  <parameter name="LinkToClick"/>
  <parameter name="ElementToVerify"/>
  <parameter name="UrlToPreviousPage"/>
  <parameter name="CheckTitle"/>
  <parameter name="ElementToClickForPreviousPage"/>
  <module name="library.function.FClickAndWaitForPageLoad">
    <parameter name="clickElement" value="@{LinkToClick}"/>
  </module>
  <action name="BUILDFAIL"/>
  <command name="assertTitle" value="regexp:^@{CheckTitle}.*"/>
  <command name="waitForElementPresent" target="@{ElementToVerify}"/>
  <command name="clickAndWait" target="@{ElementToClickForPreviousPage}"/>
</scriptmodule>