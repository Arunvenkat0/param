<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule version="3" xmlns="http://xlt.xceptance.com/xlt-script/2">
  <description>Enters event and participant data into fields when creating a new gift registry.</description>
  <command name="select" target="id=dwfrm_giftregistry_event_type" value="value=${EventType}"/>
  <command name="type" target="id=dwfrm_giftregistry_event_name" value="${EventName}"/>
  <command name="type" target="id=dwfrm_giftregistry_event_date" value="${EventDate}"/>
  <command name="select" target="id=dwfrm_giftregistry_event_eventaddress_country" value="label=${EventCountry}"/>
  <command name="select" target="id=dwfrm_giftregistry_event_eventaddress_states_state" value="value=${EventState}"/>
  <command name="type" target="id=dwfrm_giftregistry_event_town" value="${EventCity}"/>
  <command name="select" target="id=dwfrm_giftregistry_event_participant_role" value="value=${FirstParticipantRole}"/>
  <command name="select" target="id=dwfrm_giftregistry_event_coParticipant_role" value="value=${SecondParticipantRole}"/>
  <command name="type" target="id=dwfrm_giftregistry_event_coParticipant_firstName" value="${SecondParticipantFirstName}"/>
  <command name="type" target="id=dwfrm_giftregistry_event_coParticipant_lastName" value="${SecondParticipantLastName}"/>
</scriptmodule>