<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="4">
  <description>Enters event and participant data into fields when creating a new gift registry.</description>
  <module name="library.function.FSelect">
    <parameter name="element" value="id=dwfrm_giftregistry_event_type"/>
    <parameter name="selection" value="value=${EventType}"/>
  </module>
  <module name="library.function.FType">
    <parameter name="typeElement" value="id=dwfrm_giftregistry_event_name"/>
    <parameter name="inputString" value="${EventName}"/>
  </module>
  <module name="library.function.FType">
    <parameter name="typeElement" value="id=dwfrm_giftregistry_event_date"/>
    <parameter name="inputString" value="${EventDate}"/>
  </module>
  <module name="library.function.FSelect">
    <parameter name="element" value="id=dwfrm_giftregistry_event_eventaddress_country"/>
    <parameter name="selection" value="label=${EventCountry}"/>
  </module>
  <module name="library.function.FSelect">
    <parameter name="element" value="id=dwfrm_giftregistry_event_eventaddress_states_state"/>
    <parameter name="selection" value="value=${EventState}"/>
  </module>
  <module name="library.function.FType">
    <parameter name="typeElement" value="id=dwfrm_giftregistry_event_town"/>
    <parameter name="inputString" value="${EventCity}"/>
  </module>
  <module name="library.function.FSelect">
    <parameter name="element" value="id=dwfrm_giftregistry_event_participant_role"/>
    <parameter name="selection" value="value=${FirstParticipantRole}"/>
  </module>
  <module name="library.function.FSelect">
    <parameter name="element" value="id=dwfrm_giftregistry_event_coParticipant_role"/>
    <parameter name="selection" value="value=${SecondParticipantRole}"/>
  </module>
  <module name="library.function.FType">
    <parameter name="typeElement" value="id=dwfrm_giftregistry_event_coParticipant_firstName"/>
    <parameter name="inputString" value="${SecondParticipantFirstName}"/>
  </module>
  <module name="library.function.FType">
    <parameter name="typeElement" value="id=dwfrm_giftregistry_event_coParticipant_lastName"/>
    <parameter name="inputString" value="${SecondParticipantLastName}"/>
  </module>
</scriptmodule>