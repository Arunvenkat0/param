<?xml version="1.0" encoding="UTF-8" ?>
<scriptmodule version="3" xmlns="http://xlt.xceptance.com/xlt-script/2">
  <description>If the user has no default address or no saved addresses, these fields are blank.</description>
  <command name="assertText" target="xpath=//input[contains(@id, 'firstname') or contains(@id, 'firstName')]" value=""/>
  <command name="assertText" target="xpath=//input[contains(@id, 'lastname') or contains(@id, 'lastName')]" value=""/>
  <command name="assertText" target="xpath=//input[contains(@id, 'address1')]" value=""/>
  <command name="assertText" target="xpath=//select[contains(@id, 'country')]" value="regexp:Select...*"/>
  <command name="assertText" target="xpath=//select[contains(@id, 'state')]" value="regexp:Select...*"/>
  <command name="assertText" target="xpath=//input[contains(@id, 'city')]" value=""/>
  <command name="assertText" target="xpath=//input[contains(@id, 'zip')]" value=""/>
  <command name="assertText" target="xpath=//input[contains(@id, 'phone')]" value=""/>
</scriptmodule>
