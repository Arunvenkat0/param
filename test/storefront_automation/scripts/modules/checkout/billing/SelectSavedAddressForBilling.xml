<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="4">
  <description>Uses an already saved billing address while placing an order</description>
  <parameter name="AddressName"/>
  <module name="library.function.FSelectAndVerifySelection">
    <parameter name="selectElement" value="id=dwfrm_billing_addressList"/>
    <parameter name="selectLabel" value="label=(@{AddressName})*"/>
    <parameter name="labelValue" value="(@{AddressName})*"/>
  </module>
</scriptmodule>