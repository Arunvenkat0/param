<?xml version="1.0" encoding="UTF-8" ?>
<scriptmodule version="3" xmlns="http://xlt.xceptance.com/xlt-script/2">
  <description>Uses an already saved billing address while placing an order</description>
  <parameter name="AddressName"/>
  <module name="library.function.FSelectAndVerifySelection">
    <parameter name="selectElement" value="id=dwfrm_billing_addressList"/>
    <parameter name="selectLabel" value="label=(@{AddressName})*"/>
    <parameter name="labelValue" value="(@{AddressName})*"/>
  </module>
  <module name="library.function.FSelect" disabled="true">
    <parameter name="element" value="id=dwfrm_billing_addressList"/>
    <parameter name="selection" value="label=(@{AddressName})*"/>
  </module>
  <command name="select" disabled="true" target="id=dwfrm_billing_addressList" value="label=(@{AddressName})*">
    <comment>Select Address Name</comment>
  </command>
  <command name="waitForSelectedLabel" disabled="true" target="id=dwfrm_billing_addressList" value="(@{AddressName})*"/>
</scriptmodule>
