<?xml version="1.0" encoding="UTF-8" ?>
<scriptmodule version="3" xmlns="http://xlt.xceptance.com/xlt-script/2">
  <description>Uses a saved address for shipping while placing an order.</description>
  <parameter name="AddressName"/>
  <module name="library.function.FSelectAndVerifySelection">
    <parameter name="selectElement" value="id=dwfrm_singleshipping_addressList"/>
    <parameter name="selectLabel" value="label=(@{AddressName})*"/>
    <parameter name="labelValue" value="(@{AddressName})*"/>
  </module>
  <command name="select" disabled="true" target="id=dwfrm_singleshipping_addressList" value="label=(@{AddressName})*">
    <comment>Select Address from List</comment>
  </command>
  <command name="waitForSelectedLabel" disabled="true" target="id=dwfrm_singleshipping_addressList" value="(@{AddressName})*"/>
</scriptmodule>
