<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="4">
  <description>1. Waits for 'checkElement' to exist.  2. Un checks the 'checkElement'.</description>
  <parameter name="checkElement"/>
  <module name="library.function.FWaitForElement">
    <parameter name="element" value="@{checkElement}"/>
  </module>
  <command name="uncheck" target="@{checkElement}"/>
</scriptmodule>