<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="4">
  <description>1. Waits for 'textElement' to exist. 2. Waits for 'text' in 'textElement'.</description>
  <parameter name="textElement"/>
  <parameter name="text"/>
  <module name="library.function.FWaitForElement">
    <parameter name="element" value="@{textElement}"/>
  </module>
  <command name="waitForText" target="@{textElement}" value="@{text}"/>
</scriptmodule>