<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="4">
  <description>1. Executes FMouseOver of 'mouseOverElement'  2. Clicks  'clickElement'.</description>
  <parameter name="mouseOverElement"/>
  <parameter name="clickElement"/>
  <module name="library.function.FMouseOver">
    <parameter name="element" value="@{mouseOverElement}"/>
  </module>
  <module name="library.function.FClick">
    <parameter name="clickElement" value="@{clickElement}"/>
  </module>
  <command name="click" disabled="true" target="@{clickElement}"/>
</scriptmodule>