<?xml version="1.0" encoding="UTF-8" ?>
<scriptmodule version="3" xmlns="http://xlt.xceptance.com/xlt-script/2">
  <description>1. Wait for the 'textElement' to exist. 2. Stores the text from 'textElement' into the variable name in 'textStoreVariable' name.</description>
  <parameter name="textElement"/>
  <parameter name="textStoreVariableName"/>
  <module name="library.function.FWaitForElement">
    <parameter name="element" value="@{textElement}"/>
  </module>
  <command name="storeText" target="@{textElement}" value="@{textStoreVariableName}"/>
</scriptmodule>
