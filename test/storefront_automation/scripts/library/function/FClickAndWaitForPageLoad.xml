<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="4">
  <description>1. Waits for 'clickElement'.  2. Clicks 'clickElement'. 3. Waits for a new page to load.</description>
  <parameter name="clickElement"/>
  <command name="waitForElementPresent" target="@{clickElement}"/>
  <command name="clickAndWait" target="@{clickElement}"/>
</scriptmodule>