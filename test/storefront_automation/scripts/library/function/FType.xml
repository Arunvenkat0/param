<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="4">
  <description>1. Waits for the 'typeElement' to exist. 2 clicks the 'typeElement' to set focus. 3 waits for the object again. 4 Types the 'inputString' to the 'typeElement'.</description>
  <parameter name="typeElement"/>
  <parameter name="inputString"/>
  <module name="library.function.FClick">
    <parameter name="clickElement" value="@{typeElement}"/>
  </module>
  <command name="waitForElementPresent" target="@{typeElement}"/>
  <command name="pause" value="750"/>
  <command name="type" target="@{typeElement}" value="@{inputString}"/>
</scriptmodule>