<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="4">
  <description>1. Waits for 'clickElement'.  2. Clicks 'clickElement'. 3. waits for 'notElement' to not exist.</description>
  <parameter name="clickElement"/>
  <parameter name="notElement"/>
  <module name="library.function.FClick">
    <parameter name="clickElement" value="@{clickElement}"/>
  </module>
  <command name="pause" value="750"/>
  <module name="library.function.FWaitForNotElement">
    <parameter name="element" value="@{notElement}"/>
  </module>
</scriptmodule>