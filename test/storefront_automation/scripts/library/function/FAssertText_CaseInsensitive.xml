<?xml version="1.0" encoding="UTF-8" ?>
<scriptmodule version="3" xmlns="http://xlt.xceptance.com/xlt-script/2">
  <parameter name="textElement"/>
  <parameter name="string"/>
  <command name="storeEval" disabled="true" target="@{string}" value="REGEXVAR"/>
  <command name="storeEval" target="'@{string}'.replace(/[$-\/?[-^{|}]/g, '\\$&amp;')" value="REGEXVAR"/>
  <module name="library.function.FAssertText">
    <parameter name="textElement" value="@{textElement}"/>
    <parameter name="textPattern" value="regexpi:${REGEXVAR}"/>
  </module>
</scriptmodule>
