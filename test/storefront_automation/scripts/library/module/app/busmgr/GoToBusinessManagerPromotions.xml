<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="4">
  <description>Logs into Business Manager and navigates Site &gt; 'SiteName' &gt; Online Marketing &gt; Promotions</description>
  <parameter name="SiteName"/>
  <module name="library.module.app.busmgr.LoginBusinessManager"/>
  <module name="library.module.app.busmgr.Open_Site">
    <parameter name="SiteName" value="@{SiteName}"/>
  </module>
  <module name="library.module.app.busmgr.Open_OnlineMarketing"/>
  <module name="library.module.app.busmgr.Open_Promotions"/>
</scriptmodule>