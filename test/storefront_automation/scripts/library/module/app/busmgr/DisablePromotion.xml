<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="4">
  <description>Uses Business Manager to Diasble the given 'PromotionName' for the given 'SiteName'.</description>
  <parameter name="SiteName"/>
  <parameter name="PromotionName"/>
  <module name="library.module.app.busmgr.GoToBusinessManagerPromotions">
    <parameter name="SiteName" value="@{SiteName}"/>
  </module>
  <module name="library.module.app.busmgr.site.promotions.DisableAPromotion">
    <parameter name="PromotionName" value="@{PromotionName}"/>
  </module>
  <action name="Logout"/>
  <module name="library.module.app.busmgr.LogoutBusinessManager"/>
</scriptmodule>