<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="4">
  <description>From the homepage, opens the 'Level1CategoryName',  the 'Level2CategoryName', then the 'Level3CategoryName'  Example "Mens" &gt; "Clothing" &gt; "Suits"</description>
  <parameter name="Level1CategoryName"/>
  <parameter name="Level2CategoryName"/>
  <parameter name="Level3CategoryName"/>
  <module name="library.module.app.sitegenesis.util.FOpenCatalogLevel1">
    <parameter name="CategoryName" value="@{Level1CategoryName}"/>
  </module>
  <module name="library.module.app.sitegenesis.util.FOpenCatalogLevel2">
    <parameter name="CategoryName" value="@{Level2CategoryName}"/>
  </module>
  <module name="library.module.app.sitegenesis.util.FOpenCatalogLevel3">
    <parameter name="CategoryName" value="@{Level3CategoryName}"/>
  </module>
</scriptmodule>