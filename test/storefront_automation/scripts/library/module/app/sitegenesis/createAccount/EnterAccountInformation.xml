<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="4">
  <parameter name="firstName"/>
  <parameter name="lastName"/>
  <parameter name="email"/>
  <parameter name="password"/>
  <action name="VerifyCorrectPage"/>
  <module name="library.function.FWaitForElement">
    <parameter name="element" value="css=form#RegistrationForm"/>
  </module>
  <action name="Name"/>
  <module name="library.function.FType">
    <parameter name="typeElement" value="css=#dwfrm_profile_customer_firstname"/>
    <parameter name="inputString" value="@{firstName}"/>
  </module>
  <module name="library.function.FType">
    <parameter name="typeElement" value="css=#dwfrm_profile_customer_lastname"/>
    <parameter name="inputString" value="@{lastName}"/>
  </module>
  <action name="EMail"/>
  <module name="library.function.FType">
    <parameter name="typeElement" value="css=#dwfrm_profile_customer_email"/>
    <parameter name="inputString" value="@{email}"/>
  </module>
  <module name="library.function.FType">
    <parameter name="typeElement" value="css=#dwfrm_profile_customer_emailconfirm"/>
    <parameter name="inputString" value="@{email}"/>
  </module>
  <action name="Password"/>
  <module name="library.function.FType">
    <parameter name="typeElement" value="css=#dwfrm_profile_login_password"/>
    <parameter name="inputString" value="@{password}"/>
  </module>
  <module name="library.function.FType">
    <parameter name="typeElement" value="css=#dwfrm_profile_login_passwordconfirm"/>
    <parameter name="inputString" value="@{password}"/>
  </module>
</scriptmodule>