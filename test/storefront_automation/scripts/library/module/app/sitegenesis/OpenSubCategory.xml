<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="4">
  <parameter name="MainCategoryName"/>
  <parameter name="SubCategoryName"/>
  <module name="library.function.FMouseOver">
    <parameter name="element" value="//div[contains(@id,'navigation')]//a[(contains(text(),'@{MainCategoryName}') and (contains(@class,'level-1')))]"/>
  </module>
  <module name="library.function.FClick">
    <parameter name="clickElement" value="//div[contains(@id,'navigation')]//a[(contains(text(),'@{SubCategoryName') and (contains(@class,'level-2')))]"/>
  </module>
</scriptmodule>