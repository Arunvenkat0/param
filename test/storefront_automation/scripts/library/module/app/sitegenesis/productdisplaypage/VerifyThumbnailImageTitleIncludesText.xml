<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="4">
  <description>Verifies 'text' is included in the selected thumbnail image title.</description>
  <parameter name="text"/>
  <module name="library.function.FWaitForElement">
    <parameter name="element" value="css=  li.thumb.selected  img.productthumbnail[title*=&quot;@{text}&quot;]"/>
  </module>
  <module name="library.function.FAssertText" disabled="true">
    <parameter name="textElement" value="css="/>
    <parameter name="textPattern" value="@{text}"/>
  </module>
</scriptmodule>