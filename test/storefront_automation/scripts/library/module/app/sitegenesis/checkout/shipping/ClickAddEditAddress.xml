<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="4">
  <description>Clicks the "Add/Edit Adress" link for the 'ItemListNumber' ( 1,2,3,...)</description>
  <parameter name="ItemListNumber"/>
  <module name="library.function.FClick">
    <parameter name="clickElement" value="css=.item-list &gt; tbody &gt; tr:nth-child(@{ItemListNumber}) &gt; td.shippingaddress a.edit"/>
  </module>
  <module name="library.function.FWaitForElement">
    <parameter name="element" value="css=.ui-dialog-titlebar"/>
  </module>
</scriptmodule>