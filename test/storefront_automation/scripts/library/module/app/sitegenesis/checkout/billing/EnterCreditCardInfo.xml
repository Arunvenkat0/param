<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="4">
  <parameter name="NameOnCard"/>
  <parameter name="CardType"/>
  <parameter name="CardNumber"/>
  <parameter name="ExpirationMonth"/>
  <parameter name="ExpirationYear"/>
  <parameter name="SecurityCode"/>
  <action name="NameOnCard"/>
  <module name="library.function.FType">
    <parameter name="typeElement" value="${NameOnCardElement}"/>
    <parameter name="inputString" value="@{NameOnCard}"/>
  </module>
  <action name="CardType"/>
  <module name="library.function.FSelect">
    <parameter name="element" value="${CardTypeElement}"/>
    <parameter name="selection" value="@{CardType}"/>
  </module>
  <action name="CardNumber"/>
  <module name="library.function.FType">
    <parameter name="typeElement" value="${CardNumberElement}"/>
    <parameter name="inputString" value="@{CardNumber}"/>
  </module>
  <action name="ExpirationMonth"/>
  <module name="library.function.FSelect">
    <parameter name="element" value="${ExpirationMonthElement}"/>
    <parameter name="selection" value="@{ExpirationMonth}"/>
  </module>
  <action name="ExpirationYear"/>
  <module name="library.function.FSelect">
    <parameter name="element" value="${ExpirationYearElement}"/>
    <parameter name="selection" value="@{ExpirationYear}"/>
  </module>
  <action name="SecurityCode"/>
  <module name="library.function.FType">
    <parameter name="typeElement" value="${SecurityCodeElement}"/>
    <parameter name="inputString" value="@{SecurityCode}"/>
  </module>
</scriptmodule>