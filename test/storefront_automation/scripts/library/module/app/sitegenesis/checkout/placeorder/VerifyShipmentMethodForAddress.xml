<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="4">
  <description>Verifies that the shipment for 'street' uses 'shipMethod'</description>
  <parameter name="street"/>
  <parameter name="shipMethod"/>
  <module name="library.function.FAssertText">
    <parameter name="textElement" value="//div[contains(@class,&quot;mini-shipment&quot;)] // div[@class=&quot;details&quot;] / div[2][text()=&quot;@{street}&quot;] / parent::div /div[@class=&quot;minishipments-method&quot;] / span[2]"/>
    <parameter name="textPattern" value="@{shipMethod}"/>
  </module>
</scriptmodule>