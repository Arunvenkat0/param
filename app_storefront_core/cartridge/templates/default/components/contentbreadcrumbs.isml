<iscontent type="text/html" charset="UTF-8" compact="true"/>

<div class="breadcrumb">
	<isloop iterator="${pdict.Meta.getBreadcrumbs()}" alias="Item">
        <isif condition="${Item.url}">
	        <a href="${Item.url}" class="breadcrumb-element" title="${Item.name}">${Item.name}</a>
        <iselse>
            <span>${item.name}</span>
        </isif>
    </isloop>
</div><!-- END: breadcrumb -->
